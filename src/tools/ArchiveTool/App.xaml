<Application
    x:Class="ArchiveTool.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:ArchiveTool" 
    xmlns:convertersWinUI="using:ArchiveTool.ValueConverters.WinUI">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
            </ResourceDictionary.MergedDictionaries>
            <!-- Other app resources here -->
            
            <convertersWinUI:WinUIControlEventArgsConverter x:Key="WinUIControlEventArgsConverter" />
            <convertersWinUI:SelectionChangedConverter x:Key="SelectionChangedConverter" />
            
        </ResourceDictionary>
    </Application.Resources>
</Application>
